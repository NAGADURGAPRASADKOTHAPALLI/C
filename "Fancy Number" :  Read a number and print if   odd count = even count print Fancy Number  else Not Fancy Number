//"Fancy Number" :  Read a number and print if   odd count = even count print Fancy Number  else Not Fancy Number
#include<stdio.h>
void main()
{
	int n,r,even=0,odd=0;
	printf("enter a number:");
	scanf("%d",&n);
	while(n>0)
	{
		r=n%10;
		if(r%2==0)
		even++;
		else
		odd++;
		n=n/10;
	}
	if(even==odd)
	printf("FANCY");
	else
	printf("NOT FANCY");
}
