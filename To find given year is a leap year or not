//To find given year is a leap year or not.
#include<stdio.h>
void main()
{
	int n;
	printf("enter the year:");
	scanf("%d",&n);
	if(n>=4)
	{
		if(n%4==0)
		printf("given year is leap year");
		else
		printf("given year is not a leap year");			
		
		
	}
	else
	{
		if(n>0)
		printf("given year is not a leap year");
		else
		printf("given year is not a correct year ");
	}
	
}
